    $(document).ready(function() {

        function cycle(...blocks) {
            let i = 0;
            const show = () =>
                $("#" + blocks[i++ % blocks.length] )
                    .fadeIn(400)
                    .delay(blocks[i++ % blocks.length])
                    .fadeOut(400, show);
            show();
        }
        cycle("block1", 2000, "block2", 2000, "block3", 2000, "block4", 2000, "block5", 2000);


