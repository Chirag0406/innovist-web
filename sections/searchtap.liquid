                                                                            <span class="st-checkbox"></span>
                                                                            <div class="st-filter-label-container">
                                                                                <div class="filter-label">{{ list[0] }}% And Above</div>
                                                                                <span class="st-product-number">{{getSelectedCount(filter.title, list[0])}}</span>
                                                                            </div>
                                                                        </label>
                                                                    </div>
