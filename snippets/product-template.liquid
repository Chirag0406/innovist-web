<script>

  {% comment %}
  let show_add_to_cart = true;
  {% if product.tags contains 'hair-care' %}
    {% if customer %}
     let quizId = 2;
        $.ajax({
            url: `https://api-quiz.bareanatomy.in/submit?quiz_id=${quizId}&customer_id={{ customer.id }}&profile_id=1`,
            type: "GET",
            success: function(data) {
                console.log("data=======>", data.data);
                if (data.data.is_quiz_completed!=1) {
                    show_add_to_cart = false;
                }
            }
        });
    {% else %}
        show_add_to_cart = false;
    {% endif %}
  {% endif %}

  if(show_add_to_cart){
    // show add to cart and buy now button
    $('.quiz-button-product').css('display','none');
    $('.sticky-quiz-button').attr('style','display: none !important');
    if ($(window).width() >=800 ) {
      $('.add-to-cart.hanging').attr('style','display: block !important');
      $('#hanging-checkout').attr('style','display: block !important');
    } else {
      $('#sticky-atc').attr('style','display: block !important');
      $('#sticky-buy-button').attr('style','display: block !important');
    }
  }
  {% endcomment %}

  document.addEventListener("DOMContentLoaded", function(event) {
    var source =Array.from(document.getElementsByClassName("metafield-multi_line_text_field"));
    for(let i=0;i<source.length;i++){
      source[i].innerHTML=source[i].textContent ;
    }
  });




{%- liquid
  assign variants = product.variants | json
  -%}


  const getArr=()=>{
    let arr=JSON.parse('{{variants}}' )

    let result=[];

    for(let i in arr)
    {
        result.push(arr[i].id)
    }

    return result

  }
  
  let productObj={
    product_type:['{{product.type}}'],
    product_id:['{{product.id}}'],
    variant_id:getArr(),
    image:['{{product.featured_image}}'],
    price:['{{product.price}}'],
    product_title:['{{product.title}}'],
    vendor:['{{product.vendor}}'],
    url:[window.location.origin+'{{product.url}}'],
  }


  fetch("https://api-automation.bareanatomy.in/api/webEngage/sendEvent", {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json'
    },
    body: JSON.stringify({
      userId   : "" + __st.cid,
      eventName: "Page View",
      eventData: {
          "Product Type"           : productObj.product_type,
          "Product Id"             : productObj.product_id,
          "Variant Id"             : productObj.variant_id,
          "Product Image"          : productObj.image,
          "Product price"          : productObj.price,
          "Product title"          : productObj.product_title,
          "Product vendor"         : productObj.vendor,
          "Product URL"            : productObj.url
      }
    })
  }).catch(error=>{
    console.log(error);
  });


              
</script>

