                          <button
                            type="button"
                            class="js-qty__adjust js-qty__adjust--minus"
                            aria-label="{{ 'cart.general.reduce_quantity' | t }}"
                          >
                            <svg
                              aria-hidden="true"
                              focusable="false"
                              role="presentation"
                              class="icon icon-minus"
                              viewBox="0 0 64 64"
                            >
                              <path class="cls-1" d="M55 32H9"/>
                            </svg>
                            <span class="icon__fallback-text" aria-hidden="true">&minus;</span>
                          </button>
                          <button
